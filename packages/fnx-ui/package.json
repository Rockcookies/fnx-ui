    "@types/qs": "^6.9.7",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@umijs/test": "^3.5.20",
    "gh-pages": "^3.2.3",
    "qs": "^6.10.1",
