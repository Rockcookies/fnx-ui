{
  "name": "fnx-ui",
  "version": "0.1.1",
  "description": "Lightweight Mobile UI Components built in React",
  "tags": [
    "fnx",
    "fnx-ui",
    "react",
    "react-fnx"
  ],
  "keywords": [
    "react",
    "react-ui",
    "react-component",
    "fnx",
    "fnx-ui",
    "fnx-mobile",
    "component",
    "components",
    "ui",
    "framework",
    "frontend",
    "mobile"
  ],
  "sideEffects": [
    "es/**/style/*",
    "lib/**/style/*",
    "*.css",
    "*.less"
  ],
  "homepage": "https://github.com/Rockcookies/fnx-ui",
  "bugs": "https://github.com/Rockcookies/fnx-ui/issues",
  "repository": {
    "url": "https://github.com/Rockcookies/fnx-ui",
    "type": "git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "author": "Rockcookies",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "es",
    "dist"
  ],
  "browserslist": [
    "defaults",
    "Chrome >= 51",
    "Android >= 5.0",
    "IOS >= 10.0"
  ],
  "scripts": {
    "clean": "fnx-ui-cli clean && rimraf ./dist-site ./converge",
    "dev": "fnx-ui-cli dev --port 8000",
    "build": "fnx-ui-cli build",
    "build:site": "fnx-ui-cli build-site --outDir dist-site && gh-pages -d dist-site",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "lint": "fnx-ui-cli lint"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "devDependencies": {
    "@fnx-ui/cli": "^0.0.17",
    "@swc/core": "^1.3.96",
    "@swc/jest": "^0.2.29",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.8",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vant/touch-emulator": "^1.4.0",
    "gh-pages": "^6.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "@types/react-is": "^18.2.4",
    "@types/react-transition-group": "^4.4.9",
    "rc-field-form": "^1.40.0",
    "react-is": "^18.2.0",
    "react-transition-group": "^4.4.5"
  }
}
